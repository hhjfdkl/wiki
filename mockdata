,(1, 200, 'subtopic2')
,(1, 300, 'subtopic3')
,(1, 400, 'subtopic4')
,(2, 100, 'subtopic1')
,(2, 200, 'subtopic2')
,(2, 300, 'subtopic3')
,(3, 100, 'subtopic1')
,(3, 200, 'subtopic2')
,(3, 300, 'subtopic3')
,(3, 400, 'subtopic4')
,(3, 500, 'subtopic5')
